{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "indent": ["error", 2],
        "no-console": "off",
        "no-unused-expressions": "off",
        "comma-dangle": "off",
        "prefer-arrow-callback": "off",
        "func-names": "off",
        "no-new": "off",
        "arrow-body-style": "off",
        "no-prototype-builtins": "off"
    }
}
